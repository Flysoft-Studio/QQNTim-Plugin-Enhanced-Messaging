{
    "manifestVersion": "3.0",
    "id": "enhanced-messaging",
    "name": "消息增强",
    "description": "提供复读消息、转发语音消息、撤回好友私聊消息等功能。",
    "version": "1.0.0",
    "author": "Flysoft",
    "injections": [
        {
            "type": "main",
            "script": "main.js"
        },
        {
            "type": "renderer",
            "stylesheet": "style.css",
            "script": "renderer.js"
        },
        {
            "type": "renderer",
            "page": ["settings"],
            "script": "settings.js"
        }
    ]
}
